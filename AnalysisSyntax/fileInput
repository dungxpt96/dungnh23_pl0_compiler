(* This is comment *)
Program StartMyProgram;

(* Block *)
Var a,b,c;
(* ? is valid in comment *)

Procedure gcd(a; b; var d);
Begin
    While a < b Do
        If a > b Then a := 123456 * a - b / 2
        Else b := b - a;
    d := b;
End;

beGIN
    Call Readln(a);
    Call Readln(b);
    Call gcd(a,b,c);
    Call Writeln(c);
End.
